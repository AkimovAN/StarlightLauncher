<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LauncherTheme">
        <attr name="blurAmount" format="integer" />
    </declare-styleable>

    <declare-styleable name="BlurView">
        <attr name="blurAmount" />
    </declare-styleable>

    <declare-styleable name="SectionCard">
        <attr name="title" />
    </declare-styleable>

    <declare-styleable name="AppOptionMenuItem">
        <attr name="icon" format="reference" />
        <attr name="label" format="string" />
    </declare-styleable>

    <declare-styleable name="IconButton">
        <attr name="icon" />
        <attr name="iconSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TextButton">
        <attr name="color" format="color" />
    </declare-styleable>
</resources>